<odoo>
    <data>
        <template id="ssi_my_portal" inherit_id="portal.portal_my_home">
            <xpath expr="//div[hasclass('o_portal_docs')]" position="replace">
                <div t-if="approval_role != 'teacher' and approval_role != 'admin'" class="o_portal_docs list-group"></div>
                <div class="o_portal_approval list-group"></div>
            </xpath>
        </template>
    </data>
</odoo>